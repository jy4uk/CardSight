{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "crons": [
    {
      "path": "/api/tcg/update-prices",
      "schedule": "0 0 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/tcg/update-prices",
      "destination": "/api/tcg/update-prices"
    },
    {
      "source": "/api/:match*",
      "destination": "https://cardsight-production.up.railway.app/api/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
