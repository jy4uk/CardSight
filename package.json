{
    "name": "card-pilot",
    "version": "1.0.0",
    "description": "Trading Card Inventory System",
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "cd inventory-system && npm run dev",
        "dev:frontend": "cd web && npm run dev",
        "install:all": "npm run install:backend && npm run install:frontend",
        "install:backend": "cd inventory-system && npm install",
        "install:frontend": "cd web && npm install",
        "setup": "npm run install:all && npm run setup:env",
        "setup:env": "cp .env.local.example .env.local && cp inventory-system/.env.example inventory-system/.env.local",
        "migrate": "cd inventory-system && node src/migrate.js",
        "seed": "cd inventory-system && node src/seed-data.js",
        "fetch:tcg": "npx tsx inventory-system/src/card-data/fetch-tcg-data.ts"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "concurrently": "^8.2.2",
        "tsx": "^4.21.0",
        "typescript": "^5.0.0"
    },
    "keywords": [
        "cards",
        "inventory",
        "trading",
        "react",
        "node"
    ],
    "author": "Card Sight",
    "license": "MIT",
    "dependencies": {
        "@types/bcrypt": "^6.0.0",
        "axios": "^1.13.4",
        "bcrypt": "^6.0.0",
        "dotenv": "^17.2.3",
        "driver.js": "^1.4.0",
        "pg": "^8.17.2",
        "sql-parser": "^0.5.0"
    }
}
